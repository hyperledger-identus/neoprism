{
  "name": "did-midnight-serde",
  "version": "1.0.0",
  "description": "Migration from Deno to Node.js runtime for did-midnight-serde",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "compact": "compactc --skip-zk src/did.compact src/managed/did",
    "build": "npm run compact && tsc",
    "dev": "ts-node-dev src/index.ts"
  },
  "dependencies": {
    "@midnight-ntwrk/ledger": "^4.0.0",
    "@midnight-ntwrk/compact-runtime": "^0.8.1"
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
